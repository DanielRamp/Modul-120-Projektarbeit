<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <top>
            <Label id="lblWelcome" fx:id="lblWelcome" accessibleText="Willkommen" text="Willkommen" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <font>
                  <Font size="74.0" />
               </font>
               <BorderPane.margin>
                  <Insets top="20.0" />
               </BorderPane.margin>
            </Label>
         </top>
         <center>
            <Label id="lblCategoryChoose" fx:id="lblCategoryChoose" accessibleText="Kategorien auswählen" text="Kategorien auswählen" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Italic" size="39.0" />
               </font>
               <BorderPane.margin>
                  <Insets top="15.0" />
               </BorderPane.margin>
            </Label>
         </center>
      </BorderPane>
   </top>
   <center>
      <TableView id="tblCategories" fx:id="tblCategories" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
         <BorderPane.margin>
            <Insets bottom="20.0" left="100.0" right="100.0" top="5.0" />
         </BorderPane.margin>
      </TableView>
   </center>
</BorderPane>
